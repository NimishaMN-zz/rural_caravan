<div class="content">
  <section id="content">
    <section class="main padder">
      <div class="row">
        <div class="col-lg-4">
          <h2><i class="icon-user text"></i></i>Business</h2>
        </div>
        <div class="col-lg-8 m-t-large" align="right">
          <div class="btn-group">
            <button  onclick="location.href= '<%= new_business_path %>'"; class="btn btn-success btn-lg"; disabled = "current_user.subscription_end_date.present?" > <i class="icon-plus"> </i>Add New Business</button>
            </button>
         </div>
        </div>
      </div>
 <div class="panel-body">
                   
       <div class="row">
        <div class="col-lg-12"><p></p>
          <section class="panel">
           <div class="panel-body">
              <div class="table-responsive">
                <table id="businesses" class="table table-striped m-b-none text-small">
                  <thead>
                    <tr>
                      <th>Name</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if @businesses.blank? %>
                        <%= render :partial=>"/shared/no_data_found"%>
                      <% else %>

    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.name%></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Delete', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  <% end %>
                  </tbody>
                </table>
              </div>
              <div class="text-center">
              
              </div>              
            </div>  
          </section>
        </div>
      </div>
   </section>
   </section>
  </div>
