<div class="content">
<section id="content">
  <section class="main padder">
    <div class="row">
      <div class="col-lg-7">
      
        <h2><span class="glyphicon glyphicon-user"></span> <%= (@expense.vendor.name rescue nil) %></h2>
      </div>
      <div class="col-lg-5 m-t-large" align="right">
        <%= link_to raw("<i class='icon-mail-reply'></i>Back"), expenses_url, :class=>'btn  btn-default btn-lg' %>
        <p></p>
      </div>  
    <div class="col-lg-7">
        <h4>Address: <%= @expense.vendor.full_address %></h4>
        </div>  
    </div>

    
        <div class='row'>
          <div class='col-lg-12'>
            <table  class="table table-striped b-t text-small" id="report">
              <thead>
                <tr>
                <th>Date</th>
                <th>Description</th>
                <th>Amount</th>
                <th>Status</th>
                
                </tr>
              </thead>
              <tbody>
             
                <tr>
                    <td><%= @expense.expense_date %></td>
                    <td><%= @expense.description %></td>
                    <td><%= @expense.amount %></td>
                    <td><%= @expense.expense_status %></td>
                  </tr>          
             </tbody>
            </table>
          </div>
        </div>
	

</section>
</section>
</div>