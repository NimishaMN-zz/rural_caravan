<div class="content">
  <section id="content">
    <section class="main padder">
      <div class="row">
        <div class="col-lg-4">
          <h2><i class="icon-user text"></i></i>Orders</h2>
        </div>
        <div class="col-lg-8 m-t-large" align="right">
          <div class="btn-group">
            <button  onclick="location.href= '<%= new_order_path %>'"; class="btn btn-success btn-lg" > <i class="icon-plus"> </i>Add New Order</button>
            </button>
         </div>
        </div>
      </div>
 <div class="panel-body">
        
<div class="row">
        <div class="col-lg-12"><p></p>
          <section class="panel">
           <div class="panel-body">
              <div class="table-responsive">
                <table id="orders" class="table table-striped m-b-none text-small">
                  <thead>
                    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @orders.blank? %>
                        <%= render :partial=>"/shared/no_data_found"%>
                      <% else %>
    <% @orders.each do |order| %>
      <tr>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Delete', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  <% end %>
                  </tbody>
                </table>
              </div>
              <div class="text-center">
              
              </div>              
            </div>  
          </section>
        </div>
      </div>
   </section>
   </section>
  </div>
